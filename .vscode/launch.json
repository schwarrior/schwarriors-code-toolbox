{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "JS Launch Current File",
			"type": "pwa-node",
            "request": "launch",
            "program": "${file}"
        },
        {
			"name": "PS Launch Current File",
			"type": "PowerShell",
			"request": "launch",
			"script": "${file}",
			"cwd": "${file}"
		},
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Move from Root to Library",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\util\\js-util\\move-from-root-to-library.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Rename lib .linq files to .cs.linq",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\util\\js-util\\rename-linq-to-cslinq.js"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Rename git file cmd gen",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\util\\js-util\\rename-git-file-cmd-gen.js"
        }
    ]
}